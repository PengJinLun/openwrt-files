config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/qca953x_wmac'
	option htmode	HT20

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     GL.iNet
	option encryption psk-mixed
	option key goodlife
